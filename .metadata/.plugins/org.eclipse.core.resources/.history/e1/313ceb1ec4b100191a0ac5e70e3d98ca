<mapper namespace="kr.ac.hit.myapp.board.BoardDao">
    <insert id="insert" parameterType="BoardVo">
        INSERT INTO bbs(bbs_title, bbs_content, bbs_writer) VALUES (#{bbsTitle}, #{bbsContent}, #{bbsWriter})
    </insert>
    <select id="selectList" resultType="BoardVo">
        select bbs_no, bbs_title, bbs_content, bbs_writer, bbs_reg_date from bbs
    </select>
</mapper>