<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="kr.ac.hit.myapp.board.AttachDao">
    <insert id="insert" parameterType="AttachVo">
        INSERT INTO attach(att_org_name, att_new_name, att_bbs_no) VALUES (#{attOrgname}, #{attNewName}, #{attBbsNo})
    </insert>
    <select id="select" parameterType="int" resultType="AttachVo">
        select att_no,att_org_name,att_new_name,att_bbs_no
        from attach
        where att_no = #{attNo}
    </select>
</mapper>